  "keywords": [
    "symbol",
    "dispose",
    "disposes",
    "ponyfill",
    "polyfill",
    "shim"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "check-es3-syntax-cli": "^0.1.0",
    "mocha": "^7.2.0",
    "typescript": "^2.1.4"
  }
